                .col-lg-3
                    .news
                        .news-media
                            .news-media-img.news-media-img-lg(style='background-image: url(/img/corporate/img-10.jpg);')
                        .news-content
                            .news-label
                                span.bg-pink-200.text-pink-800 BBC News
